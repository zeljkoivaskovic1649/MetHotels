<html t:type="layout" title="Dodavanje eSobe"
xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
xmlns:p="tapestry:parameter">  
    <t:zone t:id="formZone">
        <t:beaneditform object="drzava" t:zone="zoneDrzave" exclude="id" >
        </t:beaneditform>
    </t:zone>
    <t:zone t:id="zoneSoba">
        <t:grid source="drzave" t:inplace="true" exclude="id" add="edit,delete" t:options="options"
        row="onesoba">
            <p:imeSobeCell>
                <div t:id="ImeSobe" value="soba.imeSobe" t:type="jquery/inPlaceEditor" t:context="soba.id" />
            </p:imeSobeCell>
            <p:spratCell>
                <div t:id="sprat" value="soba.sprat" t:type="jquery/inPlaceEditor" t:context="soba.id" />
            </p:spratCell>
            <p:editCell>
                <t:actionlink t:id="edit" t:zone="formZone" context="onesoba">Edit</t:actionlink>
            </p:editCell>
            <p:deleteCell>
                <t:actionlink t:id="delete" t:zone="zoneDrzave" context="onesoba.id">Delete</t:actionlink>
            </p:deleteCell>
        </t:grid>
    </t:zone>
</html>